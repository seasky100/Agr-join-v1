<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fundview="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <cn.fundview.app.view.SearchTitleBar
        android:id="@+id/titleBarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        fundview:type="home" />


    <cn.fundview.app.view.home.HomeView
        android:id="@+id/homewebView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:overScrollMode="never"
        tools:ignore="UnusedAttribute" />

    <cn.fundview.app.view.msg.MsgListView
        android:id="@+id/msgWebview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:overScrollMode="never"
        android:visibility="gone"
        tools:ignore="UnusedAttribute" />

    <cn.fundview.app.view.my.MyView
        android:id="@+id/myWebview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:overScrollMode="never"
        android:visibility="gone"
        tools:ignore="UnusedAttribute" />

    <cn.fundview.app.view.MenuBar
        android:id="@+id/tabs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>